<!doctype html>
<html {% if section %} data-current="{{ section }}" {% endif %} lang="en">

{% include "components/head.njk" %}

<body>


	<!-- Navbar Start -->
	<!-- Header -->
	<header class="text-white bg-black body-font">
		<div class="container mx-auto flex flex-wrap flex-col p-1 md:flex-row items-center">
			<a class="flex items-center md:mb-0" href="/">
				<div class="absolute glowLogo bg-transparent"></div>
				<img src="/_includes/assets/images/logo.svg" alt="Gyan Buzz Logo" class="w-64">
			</a>
			<nav class="md:ml-auto flex flex-wrap mt-2 md:mt-0 items-center text-base justify-center">
				<a class="mr-5 hover:text-gyanbuzz-pink focus:text-pink-600 navIcon" href="https://ggits.org">
					<div class="w-4 md:w-6 inline-block">
						<svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 576 512">
							<path
